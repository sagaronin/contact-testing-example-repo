{
  "provider": {
    "name": "ExampleProvider"
  },
  "consumer": {
    "name": "JunitDSLJsonBodyConsumer"
  },
  "interactions": [
    {
      "description": "Query name is Bheem",
      "request": {
        "method": "GET",
        "path": "/information",
        "query": {
          "name": [
            "Bheem"
          ]
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "weakness": "Laddoos",
          "strength": "Punches",
          "nationality": "India",
          "contact": {
            "Email": "chota.bheem@dholakpur.com",
            "Phone Number": "9090950980"
          },
          "name": "Chota Bheem",
          "salary": 45000
        },
        "matchingRules": {
          "body": {
            "$.salary": {
              "matchers": [
                {
                  "match": "number"
                }
              ],
              "combine": "AND"
            },
            "$.name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.strength": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.weakness": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.nationality": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": ""
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.3"
    }
  }
}